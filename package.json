{
  "name": "videotonotion",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently -n client,server -c blue,green \"pnpm run dev:client\" \"pnpm run dev:server\"",
    "dev:client": "vite",
    "dev:server": "cd server && pnpm run dev",
    "build": "vite build",
    "build:server": "cd server && pnpm run build",
    "preview": "vite preview",
    "install:all": "pnpm install && cd server && pnpm install"
  },
  "dependencies": {
    "@google/genai": "^1.30.0",
    "lucide-react": "^0.555.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.19.1",
    "@vitejs/plugin-react": "^5.1.1",
    "concurrently": "^8.2.2",
    "typescript": "~5.8.3",
    "vite": "^6.4.1"
  }
}
